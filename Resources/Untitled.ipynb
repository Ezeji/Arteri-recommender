    "def user_ready(data):\n",
    "    test = []\n",
    "    for dt in data:\n",
    "        if dt.strip() in ['TIER 1', 'NO', 'NATIONWIDE']:\n",
    "            test.append(0)\n",
    "        elif dt.strip() in ['TIER 2', 'YES', 'LAGOS']:\n",
    "            test.append(1)\n",
    "        elif dt.strip()=='TIER 3':\n",
    "            test.append(2)\n",
    "        elif dt.strip()=='TIER 4':\n",
    "            test.append(3)\n",
    "    if test[-1]==1:\n",
    "        test.append(0)\n",
    "    else:\n",
    "        test.append(1)\n",
    "\n",
    "    return test"
   ]
